{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": "18.16.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test&& npx tsc && npm install -g db-migrate && db-migrate --env test up && jasmine && db-migrate reset -e test --config ./database.json",
    "tsc": "tsc",
    "reset": "db-migrate reset -e test --config ./database.json",
    
    "build": "npx tsc",
    "db:migrate": "npm install -g db-migrate && db-migrate up && db-migrate up",
    "start:dev": "node dist/server.js",
    "createeb": "eb create --single --keyname myKey --instance-types t2.small",
    "setenv": "set_env.bat",
    "deploy": "eb init --region us-east-1 --platform Node.js store-api-dev && eb deploy store-api-dev --label \"$(date +%Y%m%d%H%M%S)\""
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "supertest": "^6.3.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
